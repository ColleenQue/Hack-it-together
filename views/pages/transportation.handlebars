<h1>Transportation</h1>
<hr />


<div id="map"></div>
    <!--Add a script by google -->

<script async
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAoE_VQv8JQz3RfB7Va-c61kqW8ZN3ir5o&callback=initMap">
</script>

<script>
  
    // Initialize and add the map
    function initMap() {
  
         // The location of Geeksforgeeks office
        const gfg_office = {
            lat: 28.50231,
            lng: 77.40548
        };
  
     // Create the map, centered at gfg_office
        const map = new google.maps.Map(
                document.getElementById("map"), {
  
            // Set the zoom of the map
            zoom: 17.56,
            center: gfg_office,
        });
    }
    </script>

    
